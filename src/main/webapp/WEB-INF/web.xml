   </servlet>
   <servlet-mapping>
   	 <servlet-name>download</servlet-name>
   	 <url-pattern>/download</url-pattern>
   </servlet-mapping>
   <servlet>
   	 <servlet-name>ss</servlet-name>
   	 <servlet-class>com.zr.controller.FileSSController</servlet-class>
   </servlet>
